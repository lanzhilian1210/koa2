#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_pdf3json@0.2.0@pdf3json/bin/pdf2json" "$@"
  ret=$?
else 
  node  "$basedir/../../../_pdf3json@0.2.0@pdf3json/bin/pdf2json" "$@"
  ret=$?
fi
exit $ret
